@model System.Exception

<!DOCTYPE html>
<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Error</title>
</head>
<body>
    <hgroup>
        <h1>Error.</h1>
        <h2>An error occurred while processing your request.</h2>
        <h3>@Model.Message</h3>
        <h4>in @Model.Source</h4>
        <h5>@Model.StackTrace</h5>
    </hgroup>
</body>
</html>
